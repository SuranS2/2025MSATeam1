
public class Operation {
	public static void main(String[] args) {
		byte b1 = 100;
		byte b2 = 1;
		
		int b3 = b1 + b2++;
		
		System.out.println(b3);
		System.out.println(b3);
		
		long lo = 10000000000L;

		float fo = 1.2f;



		long result = (long)(lo+fo);
		System.out.println(result);
		float result = lo + fo;//ÇØ°á
	}
	
}
